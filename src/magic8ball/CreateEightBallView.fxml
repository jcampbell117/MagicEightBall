<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="magic8ball.CreateEightBallViewController">
    <stylesheets>
        <URL value="@createeightballview.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" fillWidth="false" layoutX="6.0" prefHeight="214.0" prefWidth="287.0" spacing="10.0">
         <children>
            <Label fx:id="messageLabel" alignment="CENTER" contentDisplay="CENTER" text="Ask Me Anything" />
            <TextField fx:id="questionTextField" onAction="#pressedMainButton" />
            <Button mnemonicParsing="false" onAction="#pressedMainButton" text="Ask" />
         </children>
      </VBox>
   </children>
</AnchorPane>
